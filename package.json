    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "parcel": "^2.8.2",
    "postcss": "^8.4.21",
    "postcss-nesting": "^10.2.0",
    "posthtml-include": "^1.7.4",
    "prettier": "2.8.3",
    "semantic-release": "^20.1.0"
  },
  "files": [
    "main.css",
    "*.svg"
